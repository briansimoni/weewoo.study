{
  "unstable": ["kv", "cron"],
  "tasks": {
    "check": "deno fmt --check && deno lint && deno check",
    "cli": "echo \"import '\\$fresh/src/dev/cli.ts'\" | deno run --unstable-* -A -",
    "start": "deno task dev",
    "build_old": "deno task build:css && deno run -A --unstable-kv dev.ts build",
    "build": "vite build",
    "dev": "vite",
    "preview": "deno serve -A _fresh/server.js",
    "update": "deno run -A -r jsr:@fresh/update .",
    "test": "deno test --ignore=image-converter,node_modules ./**/*.test.ts"
  },
  "lint": { "rules": { "tags": ["fresh", "recommended"] } },
  "exclude": ["**/_fresh/*", "image-converter/**/*"],
  "nodeModulesDir": "auto",
  "imports": {
    "@deno-library/logger": "jsr:@deno-library/logger@^1.1.9",
    "@fresh/plugin-vite": "jsr:@fresh/plugin-vite@^1.0.5",
    "@openai/openai": "jsr:@openai/openai@^4.102.0",
    "@std/assert": "jsr:@std/assert@^1.0.13",
    "@std/csv": "jsr:@std/csv@^1.0.5",
    "@std/expect": "jsr:@std/expect@^1.0.17",
    "@std/http": "jsr:@std/http@^1.0.13",
    "@std/testing": "jsr:@std/testing@^1.0.9",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.1.14",
    "chart.js": "npm:chart.js@^4.5.0",
    "cron-time-generator": "npm:cron-time-generator@^2.0.3",
    "dayjs": "npm:dayjs@^1.11.13",
    "dayjs-plugin-utc": "npm:dayjs-plugin-utc@^0.1.2",
    "jszip": "npm:jszip@^3.10.1",
    "preact": "npm:preact@^10.27.2",
    "lucide-preact": "npm:lucide-preact@^0.477.0",
    "$std/": "https://deno.land/std@0.216.0/",
    "sharp": "npm:sharp@^0.34.3",
    "stripe": "npm:stripe@^19.1.0",
    "tailwindcss": "npm:tailwindcss@4.1.10",
    "daisyui": "npm:daisyui@5.0.27",
    "vite": "npm:vite@^7.1.9",
    "zod": "npm:zod@^3.24.2",
    "@aws-sdk/client-s3": "npm:@aws-sdk/client-s3@^3.908.0",
    "@aws-sdk/client-sqs": "npm:@aws-sdk/client-sqs@^3.908.0",
    "@aws-sdk/client-ses": "npm:@aws-sdk/client-ses@^3.908.0",
    "winston": "npm:winston@^3.12.0",
    "winston-cloudwatch": "npm:winston-cloudwatch@^6.2.0",
    "fresh": "jsr:@fresh/core@^2.1.2",
    "@preact/signals": "npm:@preact/signals@^2.3.2",
    "oauth4webapi": "npm:oauth4webapi@^3.1.2",
    "microdiff": "npm:microdiff@^1.4.0",
    "@std/encoding": "jsr:@std/encoding@^1.0.7"
  },
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.asynciterable",
      "dom.iterable",
      "deno.ns",
      "deno.unstable"
    ],
    "jsx": "precompile",
    "jsxImportSource": "preact",
    "jsxPrecompileSkipElements": [
      "a",
      "img",
      "source",
      "body",
      "html",
      "head",
      "title",
      "meta",
      "script",
      "link",
      "style",
      "base",
      "noscript",
      "template"
    ],
    "types": [
      "vite/client"
    ]
  }
}
